{
	"name": "Maersk",
	"general": {
		"url": "https://www.maersk.com/tracking/#tracking/{container}",
		"date": "%d %b %Y",
		"time": "%H:%M",
		"datetime": "%d %b %Y %H:%M",
        "single": {
            "table": "containers",
            "query": ["year", "manifest", "detail", "container"]
        },
        "multiple": {
            "key": "tracking"
        }
	},
	"input": [
		{
			"type": "class",
			"selector": "resultError",
			"assert": false,
			"required": false
		},
		{
			"type": "class",
			"selector": "font--display-1",
			"assert": false,
			"required": false
		},
		{
			"type": "id",
			"selector": "ign-accept-cookie",
			"command": {
				"type": "click",
				"wait": false,
				"required": false
			}
		},
		{
			"type": "css",
			"selector": "a.button",
			"commands": [
				{
					"index": 0,
					"type": "click",
					"required": true,
					"wait": false
				}
			]
		}
	],
	"single": [
		{
			"type": "class",
			"selector": "font--display-1--heavy",
			"commands": [
				{
					"index": 0,
					"type": "save",
					"key": "origin_point",
					"required": true
				},
				{
					"index": 1,
					"type": "save",
					"key": "dest_point",
					"required": true
				}
			]
		},
		{
			"type": "css",
			"selector": ".expandable-table__wrapper td",
			"commands": [
				{
					"index": 1,
					"type": "tag",
					"selectort": "span",
					"commands": [
						{
							"index": 3,
							"type": "save",
							"key": "description",
							"required": false
						}
					]
				},
				{
					"index": 2,
					"type": "tag",
					"selector": "span",
					"commands": [
						{
							"index": 3,
							"type": "split",
							"delimiter": "\n",
							"commands": [
								{
									"index": 0,
									"type": "save",
									"key": "arrival_date",
									"required": true
								},
								{
									"index": 1,
									"type": "save",
									"key": "arrival_time",
									"required": false
								}
							]
						}
					]
				},
				{
					"index": 3,
					"type": "tag",
					"selector": "span",
					"commands": [
						{
							"index": 3,
							"type": "regex",
							"pattern": "(.*?) • (.*?)\n(.*?)$",
							"commands": [
								{
									"index": 0,
									"type": "save",
									"key": "last_status",
									"translate": true,
									"required": true
								},
								{
									"index": 1,
									"type": "save",
									"key": "last_location",
									"required": true
								},
								{
									"index": 2,
									"type": "save",
									"key": "last_date",
									"format": "date",
									"required": true
								}
							]
						}
					]
				}
			]
		}		
	],
	"multiple": {
		"parents": {
			"type": "class",
			"selector": "timeline__event-table"
		},
		"common": [
			{
				"type": "class",
				"selector": "timeline__event-table__cell--heading",
				"commands": [
					{
						"index": 0,
						"type": "split",
						"delimiter": "\n",
						"commands": [
							{
								"index": 0,
								"type": "save",
								"key": "location_terminal",
								"required": true
							},
							{
								"index": 1,
								"type": "save",
								"key": "location",
								"required": true
							}
						]
					}
				]
			}
		],
		"specific_parents": {
			"type": "css",
			"selector": ".timeline__event-table__row:not(:first-child)"
		},
		"specific_commands": [
			{
				"type": "attr",
				"name": "data-status",
				"commands": [
					{
						"type": "compare",
						"values": ["done", "current"],
						"success": [
							{
								"type": "save",
								"key": "is_estimated",
								"value": false
							}
						],
						"failure": null,
						"required": false
					}
				]
			},
			{
				"type": "class",
				"selector": "timeline__event-table__cell--time",
				"commands": [
					{
						"index": 0,
						"type": "split",
						"delimiter": "\n",
						"commands": [
							{
								"index": 0,
								"type": "save",
								"key": "date",
								"format": "date",
								"required": true
							},
							{
								"index": 1,
								"type": "save",
								"key": "time",
								"format": "time",
								"required": false
							}
						]
					}
				]
			},
			{
				"type": "class",
				"selector": "timeline__event-table__cell--desc",
				"commands": [
					{
						"index": 0,
						"type": "split",
						"delimiter": "\n",
						"commands": [
							{
								"index": 0,
								"type": "split",
								"delimeter": " on ",
								"commands": [
									{
										"index": 0,
										"type": "save",
										"key": "status",
										"format": "status",
										"required": true
									},
									{
										"index": 1,
										"type": "save",
										"key": "transport_ship",
										"required": false
									}
								]
							},
							{
								"index": 1,
								"type": "regex",
								"pattern": "Voyage No.(.*?)$",
								"commands": [
									{
										"index": 0,
										"type": "save",
										"key": "transport_voyage"
									}
								],
								"required": false
							}
						]
					}
				]
			},
			{
				"type": "class",
				"selector": "icon-vessel",
				"command": {
					"type": "save",
					"key": "transport_type",
					"value": "Vessel"
				},
				"required": false
			},
			{
				"type": "class",
				"selector": "icon-truck",
				"command": {
					"type": "save",
					"key": "transport_type",
					"value": "Vessel"
				},
				"required": false
			}
		],
		"overwrite": true
	},
	"translation": [
        {
            "original": "Gate out",
            "translated": "Retiro de vacío",
            "exact": true
        },
        {
            "original": "Gate in",
            "translated": "Llegada de contenedor a puerto",
            "exact": true
        },
        {
            "original": "Load",
            "translated": "Contenedor cargado en barco",
            "exact": true
        },
        {
            "original": "Discharge",
            "translated": "Descarga del barco",
            "exact": true
        }
	]
}


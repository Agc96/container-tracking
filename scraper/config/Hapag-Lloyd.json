{
    "carrier": "Hapag-Lloyd",
    "general": {
        "url": "https://www.hapag-lloyd.com/en/online-business/tracing/tracing-by-container.html?container={container}",
        "date_formats": ["%Y-%m-%d", "%H:%M", "%Y-%m-%d %H:%M"],
        "single": {
            "processed": true
        },
        "multiple": {
            "transport_type": "Truck",
            "estimated": true
        }
    },
    "input": [
        {
            "type": "id",
            "selector": "tracing_by_container_f",
            "assert": true
        },
        {
            "type": "class",
            "selector": "error",
            "assert": false,
            "required": false
        },
        {
            "type": "id",
            "selector": "hal-cookieconsent-button",
            "command": {
                "type": "click",
                "wait": false,
                "required": false
            }
        }
    ],
    "single": [
        {
            "type": "class",
            "selector": "inputNonEdit",
            "commands": [
                {
                    "index": 1,
                    "type": "save",
                    "key": "description",
                    "required": true
                },
                {
                    "index": 5,
                    "type": "regex",
                    "pattern": "The (.*?) (in|from) (.*?) (at|on) (.*?) \\.",
                    "commands": [
                        {
                            "index": 0,
                            "type": "save",
                            "key": "last_status",
                            "format": "status",
                            "required": true
                        },
                        {
                            "index": 2,
                            "type": "save",
                            "key": "last_location",
                            "format": "location",
                            "required": true
                        },
                        {
                            "index": 4,
                            "type": "save",
                            "key": "last_date",
                            "format": "date",
                            "required": true
                        }
                    ],
                    "required": false
                }
            ],
            "required": true
        }
    ],
    "multiple": {
        "parents": {
            "type": "css",
            "selector": ".hal-table tbody tr"
        },
        "single": [
            {
                "type": "tag",
                "selector": "td",
                "commands": [
                    {
                        "index": 0,
                        "type": "save",
                        "key": "status",
                        "format": "status",
                        "required": true
                    },
                    {
                        "index": 1,
                        "type": "save",
                        "key": "location",
                        "format": "location",
                        "required": true
                    },
                    {
                        "index": 2,
                        "type": "save",
                        "key": "date",
                        "format": "date",
                        "required": true
                    },
                    {
                        "index": 3,
                        "type": "save",
                        "key": "date",
                        "format": "time",
                        "required": false
                    },
                    {
                        "index": 4,
                        "type": "compare",
                        "values": "Truck",
                        "success": null,
                        "failure": [
                            {
                                "type": "save",
                                "key": "transport_type",
                                "value": "Vessel"
                            },
                            {
                                "type": "save",
                                "key": "transport_ship",
                                "required": false
                            }
                        ]
                    },
                    {
                        "index": 5,
                        "type": "save",
                        "key": "transport_voyage",
                        "required": false
                    }
                ]
            },
            {
                "type": "class",
                "selector": "strong",
                "command": {
                    "type": "save",
                    "key": "estimated",
                    "value": false
                },
                "required": false
            }
        ],
        "multiple": null
    }
}
